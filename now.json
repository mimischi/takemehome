{
    "version": 2,
    "name": "takemehome",
    "builds": [
        { "src": "package.json", "use": "@now/static-build" }
    ],
    "env": {
        "VUE_APP_API_URL": "@api-url"
    },
    "routes": [
        {"src": "^/js/(.*)", "dest": "/js/$1"},
        {"src": "^/css/(.*)", "dest": "/css/$1"},
        {"src": "^/fonts/(.*)", "dest": "/fonts/$1"},
        {"src": "^/favicon.ico", "dest": "/favicon.ico"},
        {"src": "^/(.*)", "dest": "/index.html"}
    ]
}
